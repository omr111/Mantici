
@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>ayha.net | Taze, Doğal, Hızlı ve Ekonomik</title>
    <meta name="description" content="Taze, doğal, hızlı ve ekonomik bir yemek deneyimi için hizmetinizdeyiz. Porsiyon ve paketli ürünlerimize göz atmak için sayfamıza bekleriz.">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords" content="Ayha, restoran, yemek, mantı, lezzet, taze, doğal, kahvaltı" />
    <meta name="robots" content="Ayha, restoran, yemek, mantı, lezzet, taze, doğal, kahvaltı" />
    <meta name="googlebot" content="Ayha, restoran, yemek, mantı, lezzet, taze, doğal, kahvaltı" />
   
    @{
        Html.RenderAction("favicon","Home");
    }
    <!-- Place favicon.ico in the root directory -->
    <!-- CSS here -->
  
   
   
    <link href="~/content/css/bootstrap.min.css" rel="stylesheet" />

    @RenderSection("css", false)

    <link href="~/content/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/content/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/content/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/content/css/themify-icons.css" rel="stylesheet" />
    <link href="~/content/css/nice-select.css" rel="stylesheet" />
