    <link href="~/content/css/footerCss.css" rel="stylesheet" />

  
</head>

<body>

<header>
  
    <div class="header-area">
        <div id="sticky-header" class="main-header-area">
            <div class="container col-md-12">
                <div class="row align-items-center">
                    <div class="col-xl-10 col-lg-10">
                        <div class="main-menu d-none d-lg-block">
                            <nav>
                                <ul class="mein_menu_list" id="navigation">
                                    <li style="right: 5px;"><a href="/Home/Index">Anasayfa</a></li>
                                    <li style="right: 5px;"><a href="/menu/Index">Menü</a></li>

                                    <li style="right: 5px;"><a href="/about/Index">Hakkımızda</a></li>
                                    <li style="right: 5px;"><a href="/Branch/BranchApplication">Bayi Başvuru form</a></li>
                                    @{
                                        Html.RenderAction("logo","Home");
                                    }
                                    <li style="left: 10px">
                                        <a href="/branch/Index">Bayiler</a>
                                    </li>

                                    <li style="left: 10px">
                                        <a href="/contact/Index">İletişim</a>

                                    </li>
                                    <li style="left: 10px"><a href="/services/Index">Hizmetler</a></li>
                                    @if (User.Identity.IsAuthenticated == false)
                                    {
                                        <li><a href="/user/Index">Giriş</a></li>


                                    }
                                    else
                                    {
                                        <li style="left: 10px">
                                            <a href="#">@User.Identity.Name</a>
                                            <ul class="submenu">
                                                <li><a href="/user/userProfile">Profil</a></li>
                                                <li><a href="/user/logOut">Çıkış</a></li>
                                            </ul>
                                        </li>

                                    }
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="col-xl-2 col-lg-2 d-none d-lg-block">
                        <div class="custom_order">
                            <a href="/Reservation/Index" class="boxed_btn_white" style="background-color: #78b454 !important;margin-left: 10px;">Rezervasyon</a>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="mobile_menu d-block d-lg-none"></div>
                    </div>
                    @{
                        Html.RenderAction("MobilLogo","Home");
                    }
                    
                </div>
            </div>
        </div>
    </div>
</header>
@{
    Html.RenderAction("slider","Home");
}
    <!-- header-end -->
    @RenderBody()
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    @{
        Html.RenderAction("footer","Home");
    }
   
